<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentLeft="false"
    android:background="@drawable/fondo"
    android:backgroundTintMode="add"
    android:visibility="visible"
    >

    <Button
        android:id="@+id/button"
        android:layout_width="73dp"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:background="@drawable/atras" />

    <Button
        android:id="@+id/button3"
        android:layout_width="198dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|clip_horizontal|center|center_horizontal"
        android:layout_margin="50dp"
        android:background="@drawable/registrarme" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="216dp"
        android:layout_marginEnd="0dp"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="53dp"
        android:layoutDirection="locale"
        android:src="@drawable/domiciliario" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="303dp"
        android:layout_height="48dp"
        android:layout_gravity="center"
        android:width="1dp"
        android:autoText="true"
        android:background="@drawable/stylo_bode_editex"
        android:backgroundTintMode="src_in"
        android:breakStrategy="high_quality"
        android:color="@android:color/holo_blue_dark"
        android:drawableLeft="@drawable/icononpmbre"
        android:fontFamily="sans-serif-light"
        android:foregroundTint="#C15B5B"
        android:shadowColor="#1E1B1E"
        android:text="    Nombre"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#716161"
        android:textSize="24sp"
        android:typeface="sans" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="302dp"
        android:layout_height="48dp"
        android:layout_gravity="clip_horizontal|center|center_horizontal"
        android:layout_marginBottom="-60dp"
        android:autoLink="phone"
        android:background="@drawable/stylo_bode_editex"
        android:drawableLeft="@drawable/icononpmbre"
        android:lineSpacingExtra="24sp"
        android:text="Apellido"
        android:textColor="#AC9F9F"
        android:textSize="24sp"
        tools:text="  Apellido" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="301dp"
        android:layout_height="43dp"
        android:layout_gravity="center"
        android:layout_marginBottom="-110dp"
        android:background="@drawable/stylo_bode_editex"
        android:drawableLeft="@drawable/bicicleta"
        android:lineSpacingExtra="24sp"
        android:text="       Tipo de vehiculo"
        android:textColor="#554C4C"
        android:textSize="24sp"
        android:textStyle="bold"
        android:typeface="sans"
        tools:text="  Tipo de vehiculo" />

    <Button
        android:id="@+id/button4"
        android:layout_width="34dp"
        android:layout_height="26dp"
        android:layout_gravity="center|right"
        android:layout_marginBottom="-112dp"
        android:layout_marginRight="80dp"
        android:background="@drawable/polgono" />


</FrameLayout>